{
    "name": "pgn",
    "displayName": "Portable Game Notation",
    "description": "Syntax highlighting for chess game notation files",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "pgn",
            "aliases": ["Portable Game Notation", "pgn"],
            "extensions": [".pgn"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pgn",
            "scopeName": "text.pgn",
            "path": "./syntaxes/pgn.tmLanguage.json"
        }],
        "snippets": [{
            "language": "pgn",
            "path": "./snippets.json"
        }]
    }
}
